{
	"version": 2,
	"rules": [
	  {
	    "description": "products",
	    "host": "*",
	    "http_method": "*",
	    "url_path": "product/all",
	    "fixed_target": 0,
	    "rate": 0.05
	  },
	  {
		"description": "create product",
		"host": "*",
		"http_method": "POST",
		"url_path": "product/*",
		"fixed_target": 0,
		"rate": 0.05
	      },
	  {
	    "description": "get product by id",
	    "host": "*",
	    "http_method": "GET",
	    "url_path": "product/*",
	    "fixed_target": 0,
	    "rate": 0.05
	  }
	],
	"default": {
	  "fixed_target": 1,
	  "rate": 0.1
	}
      }